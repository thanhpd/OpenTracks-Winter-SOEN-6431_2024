<?xml version="1.0" encoding="utf-8"?>


<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:title="@string/settings_profile_title">
    <EditTextPreference
        android:inputType="number"
        android:key="@string/settings_profile_nickname_key"
        android:title="@string/settings_profile_nickname"
        app:useSimpleSummaryProvider="true" />
    <ListPreference
        android:key="country_preference_key"
        android:title="Country"
        android:entries="@array/countries_array"
        android:defaultValue="Canada"
        android:entryValues="@array/countries_array_values"


        />
</PreferenceScreen>
